generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Order {
  id            String   @id @default(uuid())
  amount        Float
  network       String
  email         String
  walletAddress String
  status        String   @default("PENDING")
  createdAt     DateTime @default(now())
}
